<?xml version="1.0" encoding="utf-8"?>
<!-- 资源模块相关风格 -->
<resources>
    <style name="ResSimpleFilterItemRb">
        <item name="android:button">@null</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/sl_rb_filter_item</item>
        <item name="android:padding">8dp</item>
        <item name="android:background">?selectableItemBackground</item>
    </style>

    <style name="ResourceItemTitle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#222222</item>
        <item name="android:lines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="ResourceItemNumberTv">
        <item name="android:textSize">16sp</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="ResourceItemNumberTv.ViewCount">
        <item name="android:drawableLeft">@drawable/ic_view_count</item>
        <item name="android:drawableStart">@drawable/ic_view_count</item>
    </style>

    <style name="ResourceItemNumberTv.DownloadCount">
        <item name="android:drawableLeft">@drawable/ic_download</item>
        <item name="android:drawableStart">@drawable/ic_download</item>
    </style>

    <style name="ResourceItemNumberTv.Duration">
        <item name="android:drawableLeft">@drawable/ic_duration</item>
        <item name="android:drawableStart">@drawable/ic_duration</item>
    </style>

    <style name="SeekBarAppTheme" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressDrawable">@drawable/sb_audio_current</item>
        <item name="android:indeterminateDrawable">@drawable/sb_audio_current</item>
        <item name="android:thumb">@drawable/sb_thumb</item>
    </style>
</resources>